@model Department
@{
    ViewData["Title"] = "Create";
            // string ActionName = "Create";
}

<h1 class="mb-3">Create Department</h1>



<form asp-action="Create" method="post" class="border border-2 border-primary-subtle border-4 rounded-4 bg-primary-subtle p-5">

    <div >
        @await Html.PartialAsync("PartialViews/DepartmentEditCreatePartial", @Model)
    </div>

    <div class="mt-3">
        <input type="submit" value="Create" class="btn btn-outline-primary" />
        <a asp-action="Index" class="btn btn-outline-dark px-5">Back To List</a>

    </div>
</form>


@section ScriptValidationSection {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}