@model Department
@{
    ViewData["Title"] = "Model Details";

}

<h1 class="mb-3">Model Details</h1>

<div>
    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Code"></label>
        <input asp-for="@Model.Code" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Name"></label>
        <input asp-for="@Model.Name" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.DateOfCreation"></label>
        <input asp-for="@Model.DateOfCreation" class="form-control" readonly />
    </div>
</div>
<div class="mt-3">
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-primary">Edit</a>
    <a asp-action="Index" class="btn btn-outline-dark px-5">BAck To List</a>

</div>





