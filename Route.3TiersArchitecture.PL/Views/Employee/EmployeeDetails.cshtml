@model Employee
@{
    ViewData["Title"] = "Employee Details";
}


<h1 class="mb-3">Employee Details</h1>


<div>


    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Name"></label>
        <input asp-for="@Model.Name" class="form-control " readonly />
   
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Age"></label>
        <input asp-for="@Model.Age" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Email"></label>
        <input asp-for="@Model.Email" class="form-control" readonly />
 
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Address"></label>
        <input asp-for="@Model.Address" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.Salary"></label>
        <input asp-for="@Model.Salary" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.PhoneNumber"></label>
        <input asp-for="@Model.PhoneNumber" class="form-control" readonly />
       
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.HiringDate"></label>
        <input asp-for="@Model.HiringDate" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label class="col-form-label" asp-for="@Model.IsActive"></label>
        <input id="form-Input" asp-for="@Model.IsActive" class="form-check" readonly />

    </div>


    <div class="mt-3">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-primary">Edit</a>
        <a asp-action="Index" class="btn btn-outline-dark px-5">BAck To List</a>

    </div>


</div>



    